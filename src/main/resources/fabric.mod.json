{
  "schemaVersion": 1,
  "id": "gorelib",
  "version": "${version}",
  "name": "GoreLib",
  "description": "A sweet library for all my mods",
  "authors": [
    "MrSterner"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/gorelib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.sterner.gorelib.GoreLib"
    ],
    "client": [
      "dev.sterner.gorelib.GoreLib"
    ],
    "fabric-datagen": [
      "dev.sterner.gorelib.GoreLibDataGenerator"
    ]
  },
  "mixins": [
    "gorelib.mixins.json"
  ],
  "accessWidener": "gorelib.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": "~1.19.2",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}